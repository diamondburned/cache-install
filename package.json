  "keywords": [
    "actions",
    "nix",
    "install",
    "cache"
  ],
  "author": "Rik Huijzer",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "3.0.6",
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/io": "1.1.2"
  },
  "devDependencies": {
    "esbuild": "0.15.18",
    "prettier": "2.8.0"
  }
}
